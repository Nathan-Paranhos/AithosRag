{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --max-warnings 1000",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "check": "tsc --noEmit",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:groq": "mocha tests/groq-api.test.js --timeout 30000",
    "test:mocha": "mocha tests/**/*.test.js --timeout 30000",
    "test:mocha:watch": "mocha tests/**/*.test.js --watch --timeout 30000",
    "netlify:setup": "node scripts/netlify-env-setup.js",
    "deploy:check": "npm run test:groq && npm run build",
    "env:validate": "node -e \"require('dotenv').config(); console.log(process.env.VITE_GROQ_API_KEY ? '✅ API Key encontrada: ' + process.env.VITE_GROQ_API_KEY.substring(0,10) + '...' : '❌ API Key não encontrada')\"",
    "analyze": "npx vite-bundle-analyzer dist",
    "ci": "npm run lint && npm run check && npm run test && npm run build"
  },
  "dependencies": {
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.12",
    "groq-sdk": "^0.30.0",
    "gsap": "^3.13.0",
    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.8.2",
    "recharts": "^3.1.2",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "three": "^0.179.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/three": "^0.179.0",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.21",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "chai": "^4.3.10",
    "cssnano": "^7.1.1",
    "dotenv": "^16.3.1",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jest": "^30.1.1",
    "jest-environment-jsdom": "^30.1.1",
    "mocha": "^10.2.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.35",
    "prettier": "^3.0.0",
    "bundlesize": "^0.18.1",
    "rollup-plugin-visualizer": "^6.0.3",
    "tailwindcss": "^3.4.1",
    "ts-jest": "^29.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vite-plugin-pwa": "^1.0.3"
  }
}
